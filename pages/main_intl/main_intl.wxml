<view class="container">
  <block class='scroll-container' wx:for="{{imgs}}" wx:key="{{index}}">
    <view id="{{index}}" bindtap="toDownload">
      <image src=" https://cn.bing.com{{item.url}} ">
      </image>
    </view>
    <view class='img-description'>{{item.copyright}}
    </view>
  </block>
</view>

